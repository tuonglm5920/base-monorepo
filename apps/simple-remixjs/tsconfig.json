{
  "extends": "../../tsconfig.base.json",
  "include": ["remix.env.d.ts", "**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2019"],
    "target": "ES2019",
    "allowJs": true,
    "jsx": "react-jsx",
    // Remix takes care of building everything in `remix build`.
    "noEmit": true
  }
}
